{
  "name": "glhd/guidepost",
  "description": "",
  "keywords": [
    "laravel"
  ],
  "authors": [
    {
      "name": "Chris Morrell",
      "homepage": "http://www.cmorrell.com"
    }
  ],
  "license": "MIT",
  "require": {
    "illuminate/support": "^8|^9|^10|dev-master",
    "illuminate/database": "^8|^9|^10|dev-master",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^6.21|^7.0|^8.1",
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Glhd\\Guidepost\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Glhd\\Guidepost\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "fix-style": "vendor/bin/php-cs-fixer fix",
    "check-style": "vendor/bin/php-cs-fixer fix --diff --dry-run"
  },
  "extra": {
    "laravel": {
      "providers": [
          "Glhd\\Guidepost\\Support\\GuidepostServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
